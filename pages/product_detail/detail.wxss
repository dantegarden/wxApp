}
.mbox{
    top:0;
    background-color: rgba(0,0,0,.3);
    z-index:2;
}
.pos-f-b{
    position: fixed;
    bottom: 0;
    left:0;
    right:0;
    transition: bottom 1s;
    -moz-transition: bottom 1s; /* Firefox 4 */
    -webkit-transition: bottom 1s; /* Safari 和 Chrome */
    -o-transition: bottom 1s; /* Opera */
    z-index:2;
}
.pos-f-b-hide{
    bottom: -153px;/* 底部高度 */
}
.mboxH{display:none;}

.tabs{
    display: -webkit-box;
    display: -webkit-flex;
