/* CSS Document */
.page {
    margin: 0;
    padding: 0;
    min-height: 100%;
    font-family: "iconfont", "Microsoft YaHei" !important;
    font-size: 16px;
    overflow-x: hidden;
}
page{
    background-color: #efeef6;
}
.item {
    border-color: #ddd;
    background-color: #fff;
    color: #444;
    position: relative;
    z-index: 2;
    display: block;
    padding: 16px;
    border-width: 1px;
    border-style: solid;
    font-size: 16px;
}
.button:after {
    position: absolute;
    top: -6px;
    right: -6px;
    bottom: -6px;
    left: -6px;
    content: ' ';
}
.button-block {
    display: block;
    clear: both;
}
.button.button-positive {
    border-color: transparent;
    background-color: #387ef5;
    color: #fff;
}
.button.button-block,
.button.button-full {
    margin-top: 10px;
    margin-bottom: 10px;
}
.text-center {
    text-align: center;
}
.button-block:after {
    clear: both;
}
.backgTr {
    background-color: transparent;
}
.backgW {
    background-color: #fff!important;
}
.item.borderN,
.panel-footer.borderN {
    border: none;
}
.pd-b-10{
    padding-bottom: 10px;
}
.pd-b-20{
    padding-bottom: 20px;
}
.panel-body {
    padding: 15px;
}
.mg-t-50 {
    margin-top: 50px;
}
.panel-footer {
    padding: 10px 15px;
    background-color: #fff;
    border-top: 1px solid #ddd;
}
.panel-footer {
    border-top: 1px solid #e8ecf3;
    border-bottom-right-radius: 2px;
    border-bottom-left-radius: 2px;
    background-color: #fcfcfc;
}
.col-blue{
    color: #2aa2c1;
}
.fontW{
    font-weight: bold;
}
input,span{
    display: inline-block;
    vertical-align:middle;
}
.button-red {
    color: #ffffff;
    background-color: #ee4c44;
}
.button.button-red-clik,.button-red-clik{
    border-color: #ee4c44;
    background-color: #EE1412;
    color: #fff;
    box-shadow: inset 0px 1px 3px rgba(0, 0, 0, 0.15);
}
.button-blue{
    border-color: #2aa2c1;
    background-color: #2aa2c1;
    color: #fff;
}
.button.button-blue-clik,.button-blue-clik{
    border-color: #067fc1;
    background-color: #067fc1;
    color: #fff;
    box-shadow: inset 0px 1px 3px rgba(0, 0, 0, 0.15);
}

.pos-re{
    position: relative;
}
.tabs{
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -moz-flex;
    display: -ms-flexbox;
    display: flex;
    border-bottom: solid 1px #e5e5e5;
}
.tab-item1 {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -moz-box-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    flex: 1;
    display: block;
    overflow: hidden;
    height: 100%;
    color: inherit;
    text-align: center;
    text-decoration: none;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-weight: 400;
    font-size: 14px;
    font-family: "-apple-system", "Helvetica Neue", "Roboto", "Segoe UI", sans-serif;
    opacity: 0.7; }
.tab-item button{
    width:60%;
    background-color: transparent;
    border: none;
    box-shadow: none;
    position: inherit;
    border-radius: 0;
}
.tab-item button.active{
    border-bottom: solid 2px #2aa2c1;
}
.fonts14{
    font-size: 14px;
}
button::after{ border: none; }

.mg-t-10 {
    margin-top: 10px;
}
radius-top {
    border-top-right-radius: 8px;
    border-top-left-radius: 8px;
}
radius-bottom {
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px;
}


