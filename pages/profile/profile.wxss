    z-index: 2;
    display: block;

    padding: 16px;
    border-width: 1px;
    border-style: solid;
    font-size: 16px;
}
 .button:after {
    position: absolute;
    top: -6px;
    right: -6px;
    bottom: -6px;
    left: -6px;
